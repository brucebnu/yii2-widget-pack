{
    "name": "brucebnu/yii2-widget-pack",
    "description": "about yii2 widget whith bootstrap4\\adminlte3",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruce Bnu",
            "email": "bruce.bnu@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "^2.0.38",
        "yiisoft/yii2-bootstrap4": "~2.0.6",
        "almasaeed2010/adminlte": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "bnu\\widget\\": "src"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "extra": {
        "foxy": true,
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-pattern-skip-version": "(-build|-patch)"
    }
}
